           maxEntriesLocalHeap="128" eternal="false" overflowToDisk="false"
            timeToIdleSeconds="300" timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off"
           diskPersistent="false" statistics="false" logging="true">
    </cache>
    <cache name="TechpubsWidgetSbCache"
           maxEntriesLocalHeap="128" eternal="false" overflowToDisk="false"
           timeToIdleSeconds="300" timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off"
           diskPersistent="false" statistics="false" logging="true">
    </cache>
    <cache name="TechpubsSbCache"
           maxEntriesLocalHeap="128" eternal="false" overflowToDisk="false"
           timeToIdleSeconds="300" timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off"
           diskPersistent="false" statistics="false" logging="true">
    </cache>
    <cache name="TechpubsTocSbCache"
           maxEntriesLocalHeap="128" eternal="false" overflowToDisk="false"
           timeToIdleSeconds="300" timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off"
           diskPersistent="false" statistics="false" logging="true">
    </cache>
    <cache name="TechpubsWidgetCache"
           maxEntriesLocalHeap="128" eternal="false" overflowToDisk="false"
           timeToIdleSeconds="300" timeToLiveSeconds="300"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off"
           diskPersistent="false" statistics="false" logging="true">
    </cache>
    <cache name="TechpubsCortonaCache"
      maxEntriesLocalHeap="128" eternal="false" overflowToDisk="false"
      timeToIdleSeconds="300" timeToLiveSeconds="300"
      memoryStoreEvictionPolicy="LFU" transactionalMode="off"
      diskPersistent="false" statistics="false" logging="true">
    </cache>
</ehcache>